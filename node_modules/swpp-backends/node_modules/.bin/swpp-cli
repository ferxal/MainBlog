#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules/swpp-backends/dist/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules/swpp-backends/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules/swpp-backends/dist/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules/swpp-backends/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/swpp-backends@3.0.0-alpha.908/node_modules:/mnt/g/chenHen/Github/MainBlog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli-index.js" "$@"
else
  exec node  "$basedir/../../dist/cli-index.js" "$@"
fi
